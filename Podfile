# platform :ios, '9.0'

target 'MCC' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MCC

  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  pod 'Firebase/Database'
  
  target 'MCCTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MCCUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
